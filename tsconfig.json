{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "components/Todos.jsx", "types/todoType.js", "components/Todo.jsx", "components/Navbar.jsx", "components/Dashboard.jsx", "components/AddTodo.jsx", "app/page.jsx", "app/api/webhooks/clerk/route.js", "app/sign-in/[[...sign-in]]/page.jsx", "app/sign-up/[[...sign-up]]/page.jsx", "app/layout.jsx"],
  "exclude": ["node_modules"]
}
